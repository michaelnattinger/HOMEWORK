function saddle_path = calc_saddle_p3(kgrid,kst,num,kss,Iss,del,R)

ng = length(kgrid);
grid = linspace(0,kst,num);
for i=1:ng
    dist = 100*ones(1,num);
    for j=1:num
        k0 = grid*0 + kgrid(i);
        I0 = grid;
        for tt = 1:50
            k1 = I0 + (1-del)*k0;
            I1 = (R*I0 + k1 - kst)./(1-del);
            k0 = k1
        end
    end
end